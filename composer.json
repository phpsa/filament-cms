{
  "name": "phpsa/filament-cms",
  "description": "CMS for your Filament",
  "keywords": [
    "laravel",
    "user",
    "cli",
    "resource",
    "ui",
    "filament",
    "cms",
    "fun"
  ],
  "homepage": "https://cgs4k.nz",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Phpsa\\FilamentCms\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Craig G Smith",
      "email": "vxdhost@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": "^8.1",
    "filament/filament": "^3.0",
    "filament/spatie-laravel-tags-plugin": "^3.0",
    "spatie/laravel-package-tools": "^1.10",
    "dereuromark/media-embed": "^0.6",
    "awcodes/filament-tiptap-editor": "^3.0",
    "awcodes/filament-curator": "^3.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Phpsa\\FilamentCms\\FilamentCmsProvider"
      ]
    }
  },
  "prefer-stable": true
}
